{
    "layer": "top",
    "output": ["eDP-1"],
    "modules-left": [
        "group/cpu",
        "memory",
        "disk",
        "hyprland/workspaces",
        "group/trayDrawer"
    ],
    "modules-center": ["group/player"],
    "modules-right": [
        "clock",
        "backlight",
        "pulseaudio",
        "bluetooth",
        "group/network",
        "battery",
        "group/misc"
    ],
    "margin": "6, 6, 0, 6",
    "group/cpu": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200,
            "children-class": "cpuGroup",
            "transition-left-to-right": true
        },
        "modules": ["cpu#load", "cpu#freq"]
    },
    "group/trayDrawer": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200,
            "children-class": "trayGroup",
            "transition-left-to-right": false
        },
        "modules": ["custom/trayIconRight", "custom/trayIconLeft", "tray"]
    },
    "group/player": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200,
            "children-class": "playerGroup"
        },
        "modules": ["mpris#main", "mpris#extended"]
    },
    "group/network": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200,
            "children-class": "networkGroup",
            "transition-left-to-right": true
        },
        "modules": ["network#icon", "network#ssid"]
    },
    "group/misc": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200,
            "children-class": "miscGroup",
            "transition-left-to-right": false
        },
        "modules": [
            "custom/misc",
            "idle_inhibitor",
            "custom/clipboard",
            "custom/colourpicker",
            "power-profiles-daemon"
        ]
    },
    "cpu#load": {
        "interval": "1",
        "format": " {usage}%",
        "max-length": 6,
        "min-length": 6,
        "on-click": "foot -T btop -e btop",
        "on-click-right": "foot -T nvtop -e nvtop"
    },
    "cpu#freq": {
        "interval": "1",
        "format": "| {avg_frequency}GHz",
        "on-click": "foot -T btop -e btop",
        "on-click-right": "foot -T nvtop -e nvtop"
    },
    "memory": {
        "interval": 1,
        "format": "󰘚 {percentage}%",
        "max-length": 6,
        "min-length": 6
    },
    "disk": {
        "interval": 10,
        "format": "󰋊 {percentage_used}%",
        "max-length": 6,
        "min-length": 6,
        "on-click": "nautilus",
        "tooltip-format": "{used} used out of {total}"
    },
    "hyprland/workspaces": {
        "format": "{icon}",
        "tooltip": false,
        "all-outputs": false,
        "persistent-workspaces": {
            "1": [],
            "2": [],
            "3": [],
            "4": [],
            "5": []
        },
        "format-icons": {
            "active": "",
            "default": ""
        }
    },
    "custom/trayIconRight": {
        "format": "",
        "tooltip": false
    },
    "custom/trayIconLeft": {
        "format": "|",
        "tooltip": false
    },
    "tray": {
        //"icon-size": 20,
        "spacing": 15
    },
    "hyprland/window": {
        "format": " {}",
        "separate-outputs": true,
        "max-length": 50
    },
    "mpris#main": {
        "format": "{status_icon} {title}",
        "format-stopped": "",
        "player-icons": {
            "default": "󰐊",
            "firefox": ""
        },
        "status-icons": {
            "playing": "󰐊",
            "paused": ""
        }
        //"ignored-players": ["firefox"]
    },
    "mpris#extended": {
        "format": " - {artist}",
        "format-stopped": ""
        //"ignored-players": ["firefox"]
    },
    "clock": {
        "format": "{:%I:%M %p}",
        "format-alt": "{:%R 󰃭 %d·%m·%y}",
        "tooltip-format": "<tt>{calendar}</tt>",
        "calendar": {
            "mode": "month",
            "mode-mon-col": 3,
            "on-scroll": 1,
            "on-click-right": "mode",
            "format": {
                "months": "<span color='#d8dee9'><b>{}</b></span>",
                "weekdays": "<span color='#eceff4'><b>{}</b></span>",
                "today": "<span color='#d08770'><b>{}</b></span>"
            }
        },
        "actions": {
            "on-click-right": "mode",
            "on-click-forward": "tz_up",
            "on-click-backward": "tz_down",
            "on-scroll-up": "shift_up",
            "on-scroll-down": "shift_down"
        }
    },
    "backlight": {
        "device": "intel_backlight",
        "format": "{icon} {percent}%",
        "format-icons": ["", "", "", "", "", "", "", "", ""],
        "scroll-step": 10,
        "max-length": 7,
        "min-length": 7
    },
    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-muted": "󰸈 Off",
        "tooltip": "{desc}",
        "format-icons": {
            "headphone": "",
            "default": ["󰕿", "󰖀", "󰕾"]
        },
        "on-click-middle": "~/.scripts/system/volume-control.sh m",
        "on-scroll-up": "~/.scripts/system/volume-control.sh i",
        "on-scroll-down": "~/.scripts/system/volume-control.sh d",
        "on-click": "pavucontrol"
    },
    "bluetooth": {
        "format": " On",
        "format-disabled": "󰂲", // an empty format will hide the module
        "format-connected": " {num_connections}",
        "tooltip-format": "{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}{device_address}",
        "max-length": 5,
        "min-length": 3,
        "on-click": "blueberry"
    },
    "network#icon": {
        "interval": 1,
        "format": "{icon}",
        "tooltip-format": " {bandwidthUpBytes}  {bandwidthDownBytes}",
        "format-icons": {
            "wifi": ["󰤟", "󰤢", "󰤥", "󰤨"],
            "ethernet": "󰈀",
            "disconnected": "󰤮",
            "disabled": "󰤮"
        }
    },
    "network#ssid": {
        "interval": 1,
        "format": "{essid}",
        "format-disconnected": "disconnected",
        "format-disabled": "off",
        "tooltip-format": " {bandwidthUpBytes}  {bandwidthDownBytes}"
    },
    "battery": {
        "interval": 1,
        "format": "{icon} {capacity}%",
        "format-icons": ["󰂃", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰂁", "󰂂", "󰁹"],
        "format-charging": "󰂄 {capacity}%",
        "max-length": 6,
        "min-length": 6,
        "tooltip": false,
        "on-click": "foot -T battop -e battop",
        "on-click-right": "~/.scripts/system/power-profile.sh"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "",
            "deactivated": ""
        }
    },
    "power-profiles-daemon": {
        "format": "{icon}",
        "tooltip-format": "Power profile: {profile} ",
        "tooltip": true,
        "format-icons": {
            "default": "",
            "performance": "",
            "balanced": "",
            "power-saver": ""
        }
    },
    "custom/clipboard": {
        "format": "",
        "tooltip": false,
        "on-click": "foot -T clipse -e clipse"
    },
    "custom/colourpicker": {
        "format": "",
        "tooltip": false,
        "on-click": "~/.scripts/eyedropper.sh"
    },
    "custom/misc": {
        "format": "",
        "tooltip": false
    }
}
